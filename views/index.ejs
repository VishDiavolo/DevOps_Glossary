<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <style>
      table {
        border-collapse: collapse;
        width: 100%;
      }
      th, td {
        border: 1px solid black;
        padding: 8px;
        text-align: left;
      }
      th {
        background-color: #f2f2f2;
      }
    </style>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to DevOps Glossary</p>
    <table>
      <tr>
        <th>ID</th>
        <th>Term</th>
        <th>Description</th>
        <th>Reference</th>
      </tr>
      <% items.forEach((item) => { %>
      <tr>
       <td><%= item.id %></td>
       <td><%= item.title %></td>
       <td><%= item.description %></td>
       <td><%= item.reference %></td>
      </tr>
      <%}); %>
    </table>
  </body>
</html>
